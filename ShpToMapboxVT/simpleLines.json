{
  "id": "thinkgeo-world-streets-light",
  "version": 2,
  "owner": "ThinkGeo LLC",
  "time": "2020/02/24",

  "styles": [
    {
      "id": "point",
      "filter": "zoom > 0",
      "styles": [
        {
          "point-type": "symbol",
          "point-symbol-type": "Circle",
          "point-size": 20,
          "point-outline-color": "#000000",
          "point-outline-width": 3,
          "point-fill-color": "#FF0000"
        }
      ]
    },
    {
      "id": "line",
      "filter": "zoom > 0",
      "styles": [
        {
          "line-color": "#FF0000",
          "line-width": 1
        }
      ]
    },
    {
      "id": "polygon",
      "filter": "zoom > 0",
      "styles": [
        {
          "polygon-outline-color": "#FF0000",
          "polygon-outline-width": 2
        }
      ]
    }
  ],
  "map-layers": [
    {
      "map-layer-id": "basis",
      "style-drawing-order": [
        "polygon","line","point"
      ]
    }
  ]
}